{
  "compilerOptions": {
    "target": "ES2020",
    "useDefineForClassFields": true,
    "lib": ["DOM", "DOM.Iterable","esnext", "dom", "WebWorker"],
    "types": ["node", "jest", "@testing-library/jest-dom", "@types/jest",],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "useUnknownInCatchVariables": false,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react"
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "types.d.ts", "jest.config.ts", "src/routing/routes.js"],
  "exclude": ["node_modules", "**/?(*.)+(spec|test).[tj]s?(x)"]
}
